#include <dt-bindings/gpio/mstar.h>
#include <dt-bindings/pinctrl/mstar.h>

#include "mstar-pioneer3.dtsi"

/ {
	memory@20000000 {
		device_type = "memory";
		reg = <0x20000000 0x4000000>;
	};
};

&pinctrl_pm {
	compatible = "sstar,ssd210-pm-pinctrl";
};

&sar {
	compatible = "sstar,ssd210-sar";
	status = "okay";
};
